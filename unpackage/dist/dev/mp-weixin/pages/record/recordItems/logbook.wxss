.content.data-v-5d01c5f0 {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-image: url('https://pet-daily-zm.oss-cn-beijing.aliyuncs.com/background.png');
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  background-color: #fffce0;
  width: 100%;
  height: 100vh;
}
.log.data-v-5d01c5f0 {
  width: 90%;
  background-color: #fefefe;
  border-radius: 40rpx;
  margin-bottom: 40rpx;
}
.log-header.data-v-5d01c5f0 {
  display: flex;
}
.tips.data-v-5d01c5f0 {
  width: 120rpx;
  height: 60rpx;
  border-radius: 40rpx;
  border-bottom-left-radius: 0rpx;
  border-top-right-radius: 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 600;
  color: #fff;
}
.time.data-v-5d01c5f0 {
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 600;
  color: #754712;
  margin-left: 40rpx;
}
.log-content.data-v-5d01c5f0 {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.log-text.data-v-5d01c5f0 {
  width: 85%;
  border-radius: 40rpx;
  padding: 0rpx 20rpx;
  margin: 20rpx 0rpx;
  background-color: #f8f9f4;
}
.note.data-v-5d01c5f0 {
  margin: 20rpx 0rpx;
}
/* 新增图片容器样式 */
.image-container.data-v-5d01c5f0 {
  display: flex;
  flex-wrap: wrap;
  gap: 20rpx;
  justify-content: flex-start;
}
.preview-image.data-v-5d01c5f0 {
  flex: 0 0 calc(33.33% - 14rpx);
  width: calc(33.33% - 14rpx);
  height: 180rpx;
  border-radius: 8rpx;
  box-sizing: border-box;
  /* 处理最后一行不足三个的情况 */
}
.preview-image.data-v-5d01c5f0:nth-child(3n+2):nth-last-child(1) {
  margin-right: auto;
}
.pet-container.data-v-5d01c5f0 {
  width: 85%;
  border-radius: 40rpx;
  padding: 0rpx 0rpx 20rpx;
  font-weight: 600;
  color: #754712;
  display: flex;
  align-items: center;
}
.pet-pic.data-v-5d01c5f0 {
  display: flex;
}
.pet-image.data-v-5d01c5f0 {
  width: 80rpx;
  height: 80rpx;
  border-radius: 100rpx;
}
.data-v-5d01c5f0 .uni-navbar__header-container-inner {
  align-items: flex-end !important;
  margin-bottom: 20rpx;
}
.data-v-5d01c5f0 .uni-navbar__header-btns-left {
  align-items: flex-end !important;
  margin-bottom: 20rpx;
}
.data-v-5d01c5f0 .uni-navbar--border {
  border-bottom-color: #ffe68c !important;
}
.data-v-5d01c5f0 .uni-navbar__header {
  background-color: #ffe68c !important;
}

